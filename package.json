{
  "name": "datapack",
  "version": "1.1.17",
  "description": "Datapack is a JS library that provide high performance methods `pack` and `unpack` binary data using schema of data model. This library can be used in both NodeJS and Browser environtment.",
  "keywords": [
    "pack",
    "unpack",
    "binary",
    "package",
    "proto",
    "buffer",
    "schema"
  ],
  "main": "index.js",
  "scripts": {
    "build": "rm -rf lib && tsc && cp README.md lib/ && cp package.json lib/package.json",
    "test": "ts-mocha -p tsconfig.json src/**/*.spec.ts",
    "release": "npm version patch && git push && git push --tags",
    "coverage": "nyc ts-mocha -p tsconfig.json src/**/*.spec.ts",
    "benchmark": "ts-mocha -p tsconfig.json benchmark/benchmark.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nmhung1210/datapack.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nmhung1210/datapack/issues"
  },
  "homepage": "https://github.com/nmhung1210/datapack#readme",
  "dependencies": {
    "@types/benchmark": "^2.1.5",
    "benchmark": "^2.1.4",
    "buffer": "^6.0.3"
  },
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "nyc": "^17.1.0",
    "ts-mocha": "^9.0.2",
    "typescript": "^4.6.3"
  }
}
